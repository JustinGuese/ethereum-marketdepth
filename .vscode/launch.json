    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env" : {
                "PSQL_URI" : "postgres:postgres12@localhost:5432/postgres",
            }
        }
    ]
}